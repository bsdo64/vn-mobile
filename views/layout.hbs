<!DOCTYPE html>
<html>
  <head>
    <title>{{title}}</title>
    <!-- Mobile -->
    <meta name="viewport" content="user-scalable=no,initial-scale=1,maximum-scale=1">

      {{#if production}}

          <link rel='stylesheet' href='/vendor.css' />

      {{else}}

          <link rel="stylesheet" href="/normalize-scss/normalize.css" />
          <link rel="stylesheet" href="/semantic/semantic.min.css" />
          <link rel="stylesheet" href="/font-awesome/css/font-awesome.min.css" />
          <link rel="stylesheet" href="/medium-editor/dist/css/medium-editor.min.css" />
          <link rel="stylesheet" href="/medium-editor/dist/css/themes/default.min.css" />
          <link rel="stylesheet" href="/medium-editor-insert-plugin/dist/css/medium-editor-insert-plugin.min.css" />
          <link rel="stylesheet" href="/stylesheets/common.css" />
          <link rel="stylesheet" href="/stylesheets/react-select.min.css" />
          <link rel="stylesheet" href="/stylesheets/react-datepicker.min.css" />
          <link rel="stylesheet" href="/stylesheets/highlight.js/styles/androidstudio.css" />

      {{/if}}

    {{#if meta}}
      {{#each meta}}
        <meta property="{{name}}" content="{{content}}" />
      {{/each}}
    {{/if}}
    <meta name="google-site-verification" content="PG3GDDT3-gKmlhXvD7-VnBJ0JsX-CtYgOLHOj2bR1BU" />
  </head>
  <body>
      <div id="app">
        {{{body}}}
      </div>

      {{#if production}}

          <script src="/vendor.js"></script>
          <script type="text/javascript" src="/iamport.js"></script>
          <script src="/bundle.js"></script>

      {{else}}

          <script src="/jquery/dist/jquery.min.js"></script>
          <script src="/semantic/semantic.min.js"></script>
          <script src="/medium-editor/dist/js/medium-editor.min.js"></script>
          <script src="/handlebars/handlebars.runtime.min.js"></script>
          <script src="/jquery-sortable/source/js/jquery-sortable-min.js"></script>
          <script src="/blueimp-file-upload/js/vendor/jquery.ui.widget.js"></script>
          <script src="/blueimp-file-upload/js/jquery.iframe-transport.js"></script>
          <script src="/blueimp-file-upload/js/jquery.fileupload.js"></script>
          <script src="/medium-editor-insert-plugin/dist/js/medium-editor-insert-plugin.min.js"></script>
          <script src="/iamport.js"></script>
          <script src="http://127.0.0.1:3005/_assets/bundle-Entry.js"></script>

      {{/if}}

      <script async defer
              src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDs32BDoB4rDq4-Mz5nQE6P8LBBJWW0_DQ">
      </script>
      <script async defer src='https://www.google.com/recaptcha/api.js'></script>
  </body>
</html>
